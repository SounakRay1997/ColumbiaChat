<%= form_with(model: @room, class: "", data: {
  controller: "reset-form",
  action: "ajax:success->reset-form#reset"
} ) do |f| %>
  <%= f.text_field :name, class: "form-control", autocomplete: 'off' %>
  <%= f.select :private_room, options_for_select([["Yes", "Yes"], ["No", "No"]], ["Yes", "Yes"])%>
  <br/>
  <% if @user_names %>
   <%= f.select(:selected_users, @user_names.map {|user| [user, user]}, {:prompt => "Select username", :include_hidden => false}, {:multiple => true, :size => 5} ) %>
  <% end %>
  <br/>
  <%= f.submit data: { disable_with: false } %>

  <%= f.label :room_name, "Room Name:" %>
  <%= f.text_field :name, class: "form-control", autocomplete: 'off' %>

  <%= f.label :room_name, "Distance (feet):" %>
  <%= f.text_field :distance, class: "form-control", autocomplete: 'off' %>
  <%= f.submit data: { disable_with: false } %>
<% end %>
